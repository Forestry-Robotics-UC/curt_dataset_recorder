<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="curt_mini">

  <!-- Mesh path prefix -->
  <xacro:property name="mesh_prefix" value="package://curt_description/" />

  <!-- path to package -->
  <xacro:arg name="use_sim_time" default="False" />
  <xacro:arg name="gazebo_controllers" default="config/ros2_control_simulation.yaml" />
  <xacro:arg name="simulation" default="False" />
  <!-- Import ros2_control -->
  <xacro:include filename="ros2_control.xacro" />

  <!-- Include properties -->
  <xacro:include filename="properties.xacro" />

  <!-- Include sensors -->
  <!-- <xacro:include filename="sensors.xacro" /> -->

  <link name="base_link" />

  <!-- Chassis link at same position as base_link, as root is not allowed to have inertial
  information -->
  <link name="chassis">
    <inertial>
      <origin xyz="0.011876 0.003999 0.048645" rpy="0 0 0" />
      <mass value="9.0318" />
      <inertia ixx="0.04558" ixy="-2.4654E-05" ixz="0.0014869" iyy="0.059986" iyz="2.4934E-05"
        izz="0.09132" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/base_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
  <joint name="base_to_chassis" type="fixed">
    <parent link="base_link" />
    <child link="chassis" />
  </joint>

  <link name="front_right_wheel">
    <inertial>
      <origin xyz="-2.8086E-05 0.013016 2.8089E-05" rpy="0 0 0" />
      <mass value="0.61321" />
      <inertia ixx="0.0031957" ixy="-6.9366E-08" ixz="-8.7949E-07" iyy="0.0057902" iyz="6.9371E-08"
        izz="0.0031957" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/wheel4_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.01 0" rpy="${PI / 2} 0 0" />
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}" />
      </geometry>
    </collision>
  </link>
  <joint name="front_right_motor" type="continuous">
    <origin xyz="0.25995 -0.258 -0.030204" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="front_right_wheel" />
    <axis xyz="0 1 0" />
    <dynamics damping="1.0" />
    <limit effort="${max_effort}" velocity="${max_wheel_velocity}" />
  </joint>

  <link name="front_left_wheel">
    <inertial>
      <origin xyz="2.8086E-05 -0.013016 2.8089E-05" rpy="0 0 0" />
      <mass value="0.61321" />
      <inertia ixx="0.0031957" ixy="-6.9366E-08" ixz="8.7949E-07" iyy="0.0057902" iyz="-6.9371E-08"
        izz="0.0031957" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/wheel1_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.01 0" rpy="${PI / 2} 0 0" />
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}" />
      </geometry>
    </collision>
  </link>
  <joint name="front_left_motor" type="continuous">
    <origin xyz="0.25995 0.258 -0.030204" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="front_left_wheel" />
    <axis xyz="0 1 0" />
    <dynamics damping="1.0" />
    <limit effort="${max_effort}" velocity="${max_wheel_velocity}" />
  </joint>

  <link name="back_right_wheel">
    <inertial>
      <origin xyz="-2.8086E-05 0.013016 2.8089E-05" rpy="0 0 0" />
      <mass value="0.61321" />
      <inertia ixx="0.0031957" ixy="-6.9366E-08" ixz="-8.7949E-07" iyy="0.0057902" iyz="6.9371E-08"
        izz="0.0031957" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/wheel3_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0.01 0" rpy="${PI / 2} 0 0" />
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}" />
      </geometry>
    </collision>
  </link>
  <joint name="back_right_motor" type="continuous">
    <origin xyz="-0.26 -0.258 -0.022755" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="back_right_wheel" />
    <axis xyz="0 1 0" />
    <dynamics damping="1.0" />
    <limit effort="${max_effort}" velocity="${max_wheel_velocity}" />
  </joint>

  <link name="back_left_wheel">
    <inertial>
      <origin xyz="2.8086E-05 -0.013016 2.8089E-05" rpy="0 0 0" />
      <mass value="0.61321" />
      <inertia ixx="0.0031957" ixy="-6.9366E-08" ixz="8.7949E-07" iyy="0.0057902" iyz="-6.9371E-08"
        izz="0.0031957" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/wheel2_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 -0.01 0" rpy="${PI / 2} 0 0" />
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_width}" />
      </geometry>
    </collision>
  </link>
  <joint name="back_left_motor" type="continuous">
    <origin xyz="-0.26 0.258 -0.022755" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="back_left_wheel" />
    <axis xyz="0 1 0" />
    <dynamics damping="1.0" />
    <limit effort="${max_effort}" velocity="${max_wheel_velocity}" />
  </joint>

  <!-- Import gazebo infos -->
  <xacro:if value="$(arg simulation)">
    <xacro:include filename="gazebo.xacro" />
  </xacro:if>

  <!-- OUSTER TOWER VISUAL LINK -->
  <!-- This link contains only the visual mesh for the ouster lidar -->
  <link name="ouster_tower_visual">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/Assembly_OS1.obj" />
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- OUSTER TOWER VISUAL JOINT -->
  <!-- This joint positions the visual mesh -->
  <joint name="base_link_to_ouster_tower_visual" type="fixed">
    <parent link="base_link" />
    <child link="ouster_tower_visual" />
    <origin xyz="-0.1105 0 0.204"
            rpy="0 0 ${PI}" />
  </joint>  

  <!-- CAMERA TOWER VISUAL LINK -->
  <!-- This link contains only the visual mesh for all three cameras -->
  <link name="cameras_tower_visual">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}meshes/Assembly_Cameras.obj" />
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- CAMERA TOWER VISUAL JOINT -->
  <!-- This joint positions the visual mesh -->
  <joint name="base_link_to_cameras_tower_visual" type="fixed">
    <parent link="base_link" />
    <child link="cameras_tower_visual" />
    <origin xyz="-0.2407 0 0.204"
            rpy="0 0 ${PI}" />
  </joint>

  <!-- ========================================== -->
  <!-- OUSTER OS1 LINK TREE (CALIBRATED)          -->
  <!-- base_link -> os_sensor -> (os_lidar, os_imu) -->
  <!-- ========================================== -->

  <!-- OS_SENSOR LINK -->
  <link name="os_sensor">
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- OS_SENSOR JOINT -->
  <joint name="base_link_to_os_sensor" type="fixed">
    <parent link="base_link" />
    <child link="os_sensor" />
    <origin xyz="-0.1105 0.0 0.404" rpy="0.0 0.0 0" />
  </joint>

  <!-- OS_LIDAR LINK -->
  <link name="os_lidar">
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- OS_LIDAR JOINT -->
  <joint name="os_sensor_to_os_lidar" type="fixed">
    <parent link="os_sensor" />
    <child link="os_lidar" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint>

  <!-- OS_IMU LINK -->
  <link name="os_imu">
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- OS_IMU JOINT -->
  <joint name="os_sensor_to_os_imu" type="fixed">
    <parent link="os_sensor" />
    <child link="os_imu" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint>

  <!-- ========================================== -->
  <!-- REALSENSE LINK TREE (CALIBRATED)           -->
  <!-- base_link -> camera_link -> camera_color_optical_frame -->
  <!-- ========================================== -->

  <!-- CAMERA_LINK (RealSense Physical Mount) -->
  <link name="camera_link">
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <geometry>
        <box size="0.09 0.03 0.035"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- CAMERA_LINK JOINT -->
  <joint name="base_link_to_camera_link" type="fixed">
    <parent link="base_link" />
    <child link="camera_link" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 ${PI}" />
  </joint>

  <!-- CAMERA_COLOR_OPTICAL_FRAME LINK -->
  <!-- This is the actual optical frame for the RealSense color camera -->
  <link name="camera_color_optical_frame">
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- CALIBRATED JOINT: os_lidar -> camera_color_optical_frame -->
  <!-- This is the calibrated transform between the Ouster LiDAR and RealSense camera -->
  <joint name="joint_os_lidar_to_camera_color_optical_frame" type="fixed">
    <parent link="os_lidar" />
    <child link="camera_color_optical_frame" />
    <origin xyz="-0.07047971203783951 -0.043647424976414335 -0.11746556974885704" 
            rpy="-1.567434123604688 -0.021310026725099566 ${1.575724678749616}" />
  </joint>

  <!-- ========================================== -->
  <!-- MAPIR SURVEY3 LINK TREE                    -->
  <!-- base_link -> mapir_link                    -->
  <!-- ========================================== -->

  <!-- MAPIR_LINK (Survey3) -->
  <link name="mapir_link">
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <geometry>
        <box size="0.03 0.03 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>

  <!-- MAPIR_LINK JOINT -->
  <joint name="joint_os_lidar_to_mapir_link" type="fixed">
    <parent link="os_lidar" />
    <child link="mapir_link" />
    <origin xyz="-2.106662958227812 -0.12660079802004065 -0.4261798662202779" 
            rpy="-1.7155973620374017 0.03380963393293503 ${1.1839555185746582 + PI}" />
  </joint>

</robot>